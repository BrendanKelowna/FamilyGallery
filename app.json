{
  "expo": {
    "name": "FamilyGallery",
    "slug": "FamilyGallery",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "privacy": "unlisted",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.private.FamilyGallery",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "androidStatusBar": {
    "hidden": true
  },
  "androidNavigationBar": {
    "visible": "immersive"
  },
  "plugins": [
    [
      "expo-navigation-bar",
      {
        "position": "relative",
        "visibility": "hidden",
        "behavior": "inset-touch"
      }
    ]
  ]
}